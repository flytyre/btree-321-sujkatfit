
public class BTree {

	private int degree;
	
	public BTree(int degree) {
		this.degree = degree;
	}
	
	public void move(BTreeNode node) {
		BTreeNode parent = node.getParent();
	
		if (parent != null) {
			if (parent.getNumObjects() < degree) {
				parent.addKey(node.removeObject(degree-1)); 
			} 
		} 
	}
}
